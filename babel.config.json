{
    "presets": [
        "@babel/preset-typescript",
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": true
                }
            }
        ]
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/plugin-transform-typescript",
        "@babel/proposal-object-rest-spread"
    ],
    "sourceMaps": true,
    "compact": false,
    "comments": true
}